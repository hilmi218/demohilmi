{
  "name": "with-nx",
  "$schema": "node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "./src",
  "projectType": "application",
  "targets": {
    "start": {
      "command": "refine start"
    },
    "build": {
      "command": "refine build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "build"
      }
    }
  }
}
